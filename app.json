{
  "expo": {
    "name": "Gravestone GPS",
    "slug": "mms-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.poypoy.mmsmobile",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAwEjs6KSvKF-wLubEHVAbZNCE11FJWRsI"
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow MMS to use your location."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      ["@maplibre/maplibre-react-native"]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "googleClientId": "574017815971-i0k2fgfcp2q7v6cb9nivbdrh4sj845fq.apps.googleusercontent.com",
      "eas": {
        "projectId": "6cd6943b-5d7b-4553-9117-b537e44e123e"
      }
    }
  }
}
